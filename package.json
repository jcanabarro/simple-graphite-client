  "devDependencies": {
    "@types/chai": "^4.3.8",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.8.5",
    "@types/superagent": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.51.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "superagent": "^8.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
