{
  "author": "jcanabarro (https://github.com/jcanabarro)",
  "name": "simple-graphite-client",
  "description": "Typescript compatible library to send data to a Graphite metrics server.",
  "version": "0.2.7",
  "homepage": "https://github.com/jcanabarro/simple-graphite-client",
  "repository": {
    "type": "git",
    "url": "git://github.com/jcanabarro/simple-graphite-client.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "carbon",
    "graphite",
    "metrics",
    "typescript"
  ],
  "scripts": {
    "eslint": "eslint .",
    "eslint_fix": "eslint . --fix",
